echo "enter the side of square"
read s
echo "ener the length and breadth of the rectangle"
read leng
read brea
echo "enter the radius of the circle:"
read radius
echo "Area of square is :`expr $s \* $s`"
echo "Arrea of rectangle is:`expr $leng \* $brea`"
echo "Area of the circle is:`expr 3.14 \* radius \* radius`"
