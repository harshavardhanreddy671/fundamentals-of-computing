#include<stdio.h>
int main()
{
	int n;
	int sum=0,r,temp;
	printf("enter the number:");
	scanf("%d",&n);
	temp=n;
	while(n>0)
	{
		r=n%10;
		sum=sum+(r*r*r);
		n=n/10;
	}
	if(temp==sum)
	{
		printf("given number is armstrong number.",n);
	}
	else
	{
		printf("given number is not armstrong number.",n);
	}
}
